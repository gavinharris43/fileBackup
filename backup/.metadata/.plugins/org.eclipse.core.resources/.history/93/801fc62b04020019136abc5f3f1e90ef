package com.qa.Game;

import java.util.Scanner;

/**
 * Hello world!
 *
 */
public class Runner {

	@SuppressWarnings("resource")
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		Game game = new Game();
		System.out.prdoubleln("Enter Name?");
		String doublero = sc.nextLine();
		System.out.prdoubleln(game.doublero(doublero));
		game.start();
		game.tresure();
		game.alergyInfo();
		String input = sc.nextLine();
		System.out.prdoubleln("You have chosen " + game.alergySet(input));
		game.foundTresure();
	}
}
