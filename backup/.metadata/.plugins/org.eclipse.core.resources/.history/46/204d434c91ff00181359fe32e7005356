package qa.com.game;



public class Trap {
private int nutsPositionX;
private int nutsPositionY;
private int beesPositionX;
private int beesPositionY;
private int milkPositionX;
private int milkPositionY;

public void setNuts() {
	this.nutsPositionX= (int) ((Math.random() * ((0 - 10) + 1)) + 0);
	this.nutsPositionY= (int) ((Math.random() * ((0 - 10) + 1)) + 0);
}
public void setBees() {
	this.beesPositionX= (int) ((Math.random() * ((0 - 10) + 1)) + 0);
	this.beesPositionY= (int) ((Math.random() * ((0 - 10) + 1)) + 0);
}
public void setMilk() {
	this.milkPositionX= (int) ((Math.random() * ((0 - 10) + 1)) + 0);
	this.milkPositionY= (int) ((Math.random() * ((0 - 10) + 1)) + 0);

}

	
	
}

}
