
public class Runner {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Garage garage = new Garage(1);
		Car vehicle = new Car("PJ59 ZMZ", 2, 4);
		Van vehicle1 = new Van("PJ59 ZMH", 4, 4);

		System.out.print(garage.addVehicle(vehicle));
		System.out.print(garage.addVehicle(vehicle2));
		garage.Cost(6);
		garage.removeVehicle("PJ59 ZM");

		garage.Cost(2.3, "PJ59 ZMZ");
		garage.Cost(6);

	}

}
