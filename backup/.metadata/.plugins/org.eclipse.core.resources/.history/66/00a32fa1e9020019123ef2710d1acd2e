package com.qa.BulderPattern;

class Vehicle {
	private int wheels;
	private String registration;
	private int lengthMeaters;
	private String type;

	public Vehicle() {
	}

	public int getWheels() {
		return wheels;
	}

	public void setWheels(final int wheels) {
		this.wheels = wheels;
	}

	public String getType() {
		return type;
	}

	public void setType(final String type) {
		this.type = type;
	}

	public int getLengthMeaters() {
		return lengthMeaters;
	}

	public void setLengthMeaters(final int lengthMeaters) {
		this.lengthMeaters = lengthMeaters;
	}

	public void setRegistration(final String registration) {
		this.registration = registration;

	}

	@Override
	public String toString() {
		return "Vehicle [wheels=" + wheels + ", registration=" + registration + ", lengthMeaters=" + lengthMeaters
				+ "]";
	}

	public String getRegistration() {
		return registration;
	}

}

/**
 * The builder abstraction.
 */
